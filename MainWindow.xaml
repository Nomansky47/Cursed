<Window x:Class="Cursed.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Cursed"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="Gray">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="/Resources/cube.jpg" HorizontalAlignment="Left"></Image>
        <TextBlock Text="Заголовок" FontSize="30" VerticalAlignment="Center" HorizontalAlignment="Center"></TextBlock>
        <Grid Background="LightGray" Panel.ZIndex="-2"></Grid>
        <Grid Grid.Row="2" Background="LightGray"></Grid>
        <Frame NavigationUIVisibility="Hidden" ContentRendered="MyFrame_ContentRendered"  Grid.Row="1" x:Name="MyFrame"></Frame>
        <Button Click="GoBack" x:Name="BackButton" Content="Назад" Width="175" Height="30" HorizontalAlignment="Right" Margin="5"></Button>
    </Grid>
</Window>
