<Page x:Class="Cursed.PlanePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Cursed"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="TicketPage">

    <Grid Background="Gray">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="161*"/>
            <ColumnDefinition Width="247*"/>
            <ColumnDefinition Width="129*"/>
            <ColumnDefinition Width="61*"/>
            <ColumnDefinition Width="75*"/>
            <ColumnDefinition Width="54*"/>
            <ColumnDefinition Width="73*"/>
        </Grid.ColumnDefinitions>
        <Image></Image>
        <StackPanel x:Name="Panel1" Grid.Column="0">
        </StackPanel>
        <StackPanel x:Name="Panel2" Grid.Column="1">
        </StackPanel>
        <StackPanel x:Name="Panel3" Grid.Column="2">
        </StackPanel>
        <StackPanel x:Name="Panel4" Grid.Column="3">
        </StackPanel>
        <StackPanel x:Name="Panel5" Grid.Column="5">
        </StackPanel>
    </Grid>
</Page>
